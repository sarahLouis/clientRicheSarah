{
	"name" : "phpmv/ubiquity-codeception",
	"description" : "Make Codeception's functional tests for Ubiquity framework",
	"type" : "module",
	"keywords" : [
		"php",
		"framework",
		"mvc",
		"orm",
		"tests"
	],
	"authors" : [{
			"name" : "Jean-Christophe HERON",
			"email" : "myaddressmail@gmail.com",
			"role" : "Developer"
		}
	],
	"require" : {
		"php" : ">7.0"
	},
	"autoload" : {
		"psr-4" : {
			"Codeception\\Module\\" : "src/functional/",
			"Codeception\\Lib\\Connector\\" : "src/functional/",
			"Ubiquity\\tests\\unit\\" : "src/unit/"
		}
	},
	"require-dev" : {
		"codeception/codeception" : "^2.2",
		"phpmv/ubiquity" : "dev-master"
	},
     "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}